{
  "name": "mbclient",
  "version": "1.0.0",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "description": "Message broker library",
  "keywords": [
    "amqp",
    "message broker"
  ],
  "license": "Apache-2.0",
  "author": "Gajewski Dmitriy Yurevish <d.y.gaevskiy@tinkoff.ru>",
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "repository": "https://github.com/TinkoffCreditSystems/mbclient.git"
  },
  "scripts": {
    "cz": "git-cz",
    "clean": "rm -rf dist",
    "build": "tsc",
    "watch": "tsc -w",
    "lint": "tslint --project . \"src/**/*.ts\"",
    "check": "npm run lint && npm run test:unit",
    "test:unit": "jest --forceExit --coverage --verbose --detectOpenHandles --passWithNoTests",
    "test:ci": "npm run test:unit -- --coverageReporters=text-lcov | coveralls",
    "test:unit:watch": "jest --watch",
    "prepublishOnly": "npm run check && npm run build",
    "semantic-release": "semantic-release"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "dependencies": {
    "amqplib": "^0.5.5",
    "lodash": "^4.17.15",
    "uuid": "^3.3.3"
  },
  "devDependencies": {
    "@semantic-release/changelog": "^5.0.1",
    "@semantic-release/git": "^9.0.0",
    "@types/amqplib": "^0.5.13",
    "@types/jest": "^25.2.3",
    "@types/lodash": "^4.14.146",
    "@types/node": "^8.10.59",
    "@types/uuid": "^3.4.6",
    "coveralls": "^3.1.0",
    "cz-conventional-changelog": "^3.0.2",
    "jest": "^25.5.4",
    "semantic-release": "^17.0.8",
    "ts-jest": "^25.5.1",
    "ts-node": "^8.4.1",
    "tslint": "^5.20.1",
    "tslint-config-unional": "^0.10.0",
    "typescript": "^3.9.3"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
